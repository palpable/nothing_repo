<script type="text/javascript">
 jQuery(document).ready(function($) {
    $('a[rel*=facebox]').facebox()
})
</script>
<h6>Asset Managers</h6>
<%if @asset_manager.length>0 %>
<h4>List of the Asset Managers.</h4><br>
<ul class="newsInfo">
<% @asset_manager.each do |asset| %>
<li>
  
 <p> <%= link_to(asset.name,{:controller =>'banks',:action => 'profile_display', :id =>asset.id},:title => "Asset Manager",:rel =>'facebox'
        
      ) %></p>
</li>
<%end %>
</ul>
<%else %>
<h3>No Asset Manager Found</h3>
<%end %>